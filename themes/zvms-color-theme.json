	"tokenColors": [
		{
			"scope": [
				"meta.class",
				"meta.function",
			],
			"settings": {
				"foreground": "#1453AD",
			}
		},
		{
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#CA0808",
			}
		},
		{
			"scope": [
				"entity.name",
			],
			"settings": {
				"foreground": "#212121",
				"fontStyle": "bold",
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type",
			],
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#698654",
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#1453AD"
			}
		},
		{
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#891F1F",
			}
		},
		{
			"scope": [
				"punctuation.separator",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#212121",
			}
		},
		{
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#212121",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#212121",
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#212121",
				"fontStyle": "underline"
			}
		}
